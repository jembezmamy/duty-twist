{{#unless hideButtons}}
  {{#basic-button action="previous" class="icon-only transparent" disabled=previousDisabled}}
    <i class="icon chevron-left-grey"></i>
  {{/basic-button}}

  {{#basic-button action="next" class="icon-only transparent" disabled=nextDisabled}}
    <i class="icon chevron-right-grey"></i>
  {{/basic-button}}
{{/unless}}
